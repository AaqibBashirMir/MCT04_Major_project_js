{
    "name": "My Extention",
    "version": "0.1",
    "description": "Bookmark the YouTube time stamp",
    "permissions":["storage", "tabs"],  
    "host_permissions": ["https://*.youtube.com/*"],
    "background" :{
        "service_worker" : "bg.js"
    },
    "content_scripts" : [
        {
            "matches" : ["https://*.youtube.com/*"],
            "js" : ["contentScript.js"]
        }
    ],
    
    "action" : {
        "default_popup" : "index.html"
    },
    "manifest_version" : 3 
}